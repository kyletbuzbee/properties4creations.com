{
  "name": "properties4creations",
  "version": "2.0.0",
  "description": "Enterprise-grade static site for Properties 4 Creations",
  "scripts": {
    "start": "npx npm-run-all --parallel dev:*",
    "dev:11ty": "npx @11ty/eleventy --serve --quiet",
    "dev:css": "npx tailwindcss -i ./src/css/input.css -o ./dist/css/style.css --watch",
    "build": "npx npm-run-all build:css build:11ty",
    "build:11ty": "npx @11ty/eleventy",
    "build:css": "node_modules\\.bin\\tailwindcss.cmd -i ./src/css/input.css -o ./dist/css/style.css --minify",
    "test": "echo 'No tests specified' && exit 0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.18"
  },
  "keywords": [
    "housing",
    "veterans",
    "section8",
    "east-texas"
  ],
  "author": "Properties 4 Creations",
  "license": "MIT",
  "dependencies": {
    "tailwind": "^4.0.0"
  }
}
